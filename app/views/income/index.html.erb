<h1>Listing incomes</h1>

<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Price</th>
    <th>Category</th>
    <th>Notice</th>
    <th>Income</th>
    <th>Period</th>
    <th>Launch</th>
    <th>Finish</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @incomes.each do |income| %>
      <tr>
        <td><%= income.name %></td>
        <td><%= income.price %></td>
        <td><%= show_category_name(income.category) %></td>
        <td><%= income.notice %></td>
        <td><%= income.income %></td>
        <td><%= income.period %></td>
        <td><%= income.launch %></td>
        <td><%= income.finish %></td>
        <td><%= link_to 'Show', income %></td>
        <td><%= link_to 'Edit', "incomes/edit/#{income.id}" %></td>
        <td><%= link_to 'Destroy', income, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Income', income_new_path %>